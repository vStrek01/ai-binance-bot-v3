# Copy this file to .env and replace the placeholder values with your own credentials.
# Never commit a real key/secret to the repository.

# Binance API credentials (demo or live depending on RUN_MODE).
BINANCE_API_KEY="<YOUR_BINANCE_API_KEY>"
BINANCE_API_SECRET="<YOUR_BINANCE_API_SECRET>"

# Trading profile: backtest | dry-run | demo-live | live
RUN_MODE=backtest

# Exchange routing toggles.
# Set to true for demo/testnet usage; live mode forces this to false.
BINANCE_TESTNET=true

# Additional safety confirmations for live trading (leave unset for demo/dry-run).
BOT_LIVE_TRADING=0
BOT_CONFIRM_LIVE="YES_I_UNDERSTAND_THE_RISK"

# Optional websocket overrides; uncomment only if you know what you are doing.
# BINANCE_REST_URL="https://demo-fapi.binance.com"
# BINANCE_WS_MARKET_URL="wss://demo-stream.binancefuture.com/stream"
# BINANCE_WS_USER_URL="wss://demo-stream.binancefuture.com/ws"

# Logging verbosity for CLI + API (DEBUG, INFO, WARNING...)
LOG_LEVEL=INFO

# Observability API host binding (keep loopback in prod for safety).
BOT_API_HOST=127.0.0.1
