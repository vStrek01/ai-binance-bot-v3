# Copy this file to .env and replace the placeholder values with your DEMO FUTURES TESTNET credentials only.
# Never commit real or mainnet keys to the repository.

# Binance Futures TESTNET API credentials (demo only; never mainnet!).
BINANCE_API_KEY=<YOUR_DEMO_API_KEY>
BINANCE_API_SECRET=<YOUR_DEMO_API_SECRET>

# Trading profile enforced for demo-live rehearsals only.
RUN_MODE=demo-live

# Demo/live safety toggles (1 = enabled). Keep BINANCE_TESTNET=1 for demo futures routing.
BOT_LIVE_TRADING=1
BINANCE_TESTNET=1

# Optional websocket overrides; uncomment only if you know what you are doing.
# BINANCE_REST_URL="https://demo-fapi.binance.com"
# BINANCE_WS_MARKET_URL="wss://demo-stream.binancefuture.com/stream"
# BINANCE_WS_USER_URL="wss://demo-stream.binancefuture.com/ws"

# Logging verbosity for CLI + API (DEBUG, INFO, WARNING...)
LOG_LEVEL=INFO

# Observability API host binding (keep loopback in prod for safety).
BOT_API_HOST=127.0.0.1
